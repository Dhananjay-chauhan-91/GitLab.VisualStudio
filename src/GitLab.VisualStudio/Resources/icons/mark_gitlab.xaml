<Viewbox
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
        Width="1024" Height="1024">

    <Viewbox.Resources>
        <ResourceDictionary>
            <Color x:Key="GitHubContextMenuIconColor">#424242</Color>
            <SolidColorBrush x:Key="GitHubContextMenuIconBrush" Color="{StaticResource GitHubContextMenuIconColor}" PresentationOptions:Freeze="true" />
        </ResourceDictionary>
    </Viewbox.Resources>

    <Border BorderBrush="Transparent" BorderThickness="0">
        <Image Height="16" Width="16" Source="/GitLab.VisualStudio;component/Resources/default_org_avatar.png"/>
    </Border>
</Viewbox>